{
  "name": "mqtt-iot-server-ttgo",
  "version": "1.0.0",
  "description": "Servidor MQTT para rastreamento e monitoramento de bovinos",
  "repository": "git@github.com:PedroZago/mqtt-iot-server-ttgo.git",
  "author": "Pedro de Camargo Zago <pedro.camargozago@hotmail.com.br>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "nodemon",
    "migration:generate": "npx sequelize-cli migration:generate --name",
    "migration:up": "npx sequelize-cli db:migrate --config database/config.js",
    "migration:down": "npx sequelize-cli db:migrate:undo --config database/config.js",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed:run": "npx sequelize-cli db:seed:all --config database/config.js",
    "seed:undo": "npx sequelize-cli db:seed:undo:all --config database/config.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-basic-auth": "^1.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mqtt": "^5.10.1",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "socket.io": "^4.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/mqtt": "^2.5.0",
    "@types/node": "^22.7.5",
    "@types/pg": "^8.11.10",
    "@types/sequelize": "^4.28.20",
    "@types/socket.io": "^3.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
